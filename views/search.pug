extends layouts/main
include mixins/videoBlock

block content
    .search__header 
        h3 Searching for : #{searchingBy}
    .search__video  
        if videos.length == 0
            p No such Videos
        each item in videos
            +videoBlock({
                title : item.title,
                views : item.views,
                fileUrl : item.fileUrl,
                id:item.id
            })